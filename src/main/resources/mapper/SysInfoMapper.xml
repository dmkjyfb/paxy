<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.hpc.mapper.SysInfoMapper">

    <insert id="insert" parameterType="cn.hpc.beans.SysInfo"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO T_Sys_Info(sKey,sValue) VALUES
        (#{sKey},#{sValue})
    </insert>

</mapper>